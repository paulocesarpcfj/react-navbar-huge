{
  "name": "huge-navbar-app",
  "version": "0.0.1",
  "description": "HUGE Navbar React APP",
  "scripts": {
    "start": "npm run dev",
    "dev": "node -r babel-register ./node_modules/.bin/webpack-dev-server --host 0.0.0.0 --config ./webpack/dev.js --watch --colors",
    "build": "rimraf dist && NODE_ENV=production node -r babel-register ./node_modules/webpack/bin/webpack --config ./webpack/prod.js --progress --profile --colors",
    "test": "NODE_ENV=test ./node_modules/.bin/jest",
    "test:tdd": "npm test -- --watch",
    "test:coverage": "npm test -- --coverage",
    "coverage": "cd ./coverage/lcov-report; python -m SimpleHTTPServer 9000; cd -",
    "lint": "./node_modules/.bin/eslint src",
    "lint:fix": "npm run lint -- --fix",
    "ci": "npm test --silence && npm run lint",
    "prepush": "npm run ci",
    "prestart": "npm run build",
    "heroku-postbuild": "http-server ./dist"
  },
  "jest": {
    "setupTestFrameworkScriptFile": "./tests/setup.js"
  },
  "repository": {
    "type": "git"
  },
  "author": "Paulo Ferreira",
  "private": true,
  "dependencies": {
    "autoprefixer": "^7.1.0",
    "babel-core": "^6.24.1",
    "babel-eslint": "^7.2.3",
    "babel-jest": "^20.0.3",
    "babel-loader": "^7.0.0",
    "babel-plugin-module-resolver": "^2.7.1",
    "babel-plugin-transform-async-to-generator": "^6.24.1",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-register": "^6.24.1",
    "babel-runtime": "^6.23.0",
    "babel-types": "^6.24.1",
    "chai": "^4.1.0",
    "chai-enzyme": "^0.8.0",
    "classnames": "^2.2.5",
    "copy-webpack-plugin": "^4.0.1",
    "css-loader": "^0.28.1",
    "enzyme": "^2.8.2",
    "es6-promise": "^4.1.0",
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "^9.0.1",
    "eslint-plugin-import": "^1.10.2",
    "eslint-plugin-jsx-a11y": "^2.0.0",
    "eslint-plugin-react": "^5.2.2",
    "extract-text-webpack-plugin": "^3.0.0",
    "file-loader": "^0.11.1",
    "html-webpack-plugin": "^2.28.0",
    "http-server": "^0.10.0",
    "husky": "^0.14.3",
    "jest-cli": "^20.0.3",
    "jsdom": "^9.11.0",
    "lodash": "^4.17.4",
    "postcss-discard-duplicates": "^2.1.0",
    "postcss-discard-unused": "^2.2.3",
    "postcss-loader": "^0.9.1",
    "react": "^15.5.4",
    "react-dom": "^15.5.4",
    "react-hot-loader": "^3.0.0-beta.6",
    "react-redux": "^5.0.4",
    "react-router": "^3.0.4",
    "react-router-redux": "^4.0.8",
    "react-test-renderer": "^15.5.4",
    "redux": "^3.6.0",
    "redux-logger": "^3.0.6",
    "redux-promise-middleware": "4.2.1",
    "redux-thunk": "^2.2.0",
    "rimraf": "^2.6.1",
    "script-ext-html-webpack-plugin": "^1.8.0",
    "sinon": "^2.2.0",
    "sinon-chai": "^2.10.0",
    "sinon-stub-promise": "^4.0.0",
    "store": "^2.0.4",
    "style-loader": "^0.18.2",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.1",
    "webpack": "3.4.1",
